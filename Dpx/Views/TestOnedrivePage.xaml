<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Dpx.Views.TestOnedrivePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Login" Clicked="Button_OnClicked"></Button>
            <Label x:Name="Result"></Label>
            <Button Text="Upload" Clicked="AButton_OnClicked"></Button>
            <Button Text="Download" Clicked="BButton_OnClicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>